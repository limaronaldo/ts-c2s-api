# Server
PORT=3000
HOST=0.0.0.0
NODE_ENV=development

# Database
DB_URL=postgresql://user:password@localhost:5432/c2s

# C2S API
C2S_TOKEN=your_c2s_token
C2S_URL=https://c2s.com.br/api/v1

# Work API (Completa Buscas)
